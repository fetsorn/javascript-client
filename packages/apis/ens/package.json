{
  "name": "ens-web3api",
  "description": "ENS Web3API",
  "private": true,
  "version": "0.0.1-prealpha.21",
  "scripts": {
    "build": "npx w3 build",
    "test:env:up": "npx w3 test-env up",
    "test:env:down": "npx w3 test-env down",
    "deploy": "yarn deploy:contract && yarn deploy:web3api",
    "deploy:web3api": "npx w3 build --ipfs http://localhost:5001 --test-ens ens.eth",
    "deploy:contract": "node ./deploy-contracts.js",
    "test": "npx w3 query ./recipes/e2e.json --redirects ./redirects.js"
  },
  "devDependencies": {
    "@web3api/cli": "0.0.1-prealpha.21",
    "@web3api/ethereum-plugin-js": "0.0.1-prealpha.21",
    "@web3api/wasm-as": "0.0.1-prealpha.21",
    "ethers": "5.0.8",
    "js-yaml": "3.14.0",
    "dotenv": "8.2.0",
    "ts-node": "9.1.1"
  }
}
