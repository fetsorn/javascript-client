{
  "name": "uniswapV2-api-assemblyscript",
  "description": "UniswapV2 Web3API",
  "private": true,
  "version": "0.0.1-prealpha.8",
  "scripts": {
    "build": "npx w3 build",
    "build:as": "asc ./src/utils/BigInt.ts -o ./build/utils/BigInt.wasm -d ./build/utils/BigInt.d.ts",
    "test:as": "asp --verbose",
    "test:ci": "asp --summary",
    "test:env:up": "npx w3 test-env up",
    "test:env:down": "npx w3 test-env down"
  },
  "workspaces": {
    "nohoist": [
      "assemblyscript",
      "assemblyscript/**"
    ]
  },
  "devDependencies": {
    "@as-pect/cli": "5.0.1",
    "@uniswap/v2-core": "1.0.1",
    "@uniswap/v2-periphery": "1.1.0-beta.0",
    "@web3api/cli": "0.0.1-prealpha.11",
    "@web3api/ethereum-plugin-js": "0.0.1-prealpha.11",
    "@web3api/wasm-as": "0.0.1-prealpha.11",
    "@web3api/client-js": "0.0.1-prealpha.11",
    "assemblyscript": "0.17.14",
    "ethers": "5.0.8",
    "js-yaml": "3.14.0",
    "@types/jest": "26.0.8",
    "@types/node": "12.6.9",
    "@web3api/test-env-js": "0.0.1-prealpha.11",
    "jest": "26.6.3",
    "rimraf": "3.0.2",
    "ts-jest": "26.5.4",
    "typescript": "4.0.7"
  }
}
