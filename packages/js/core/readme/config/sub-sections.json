{
  "doc-snippets": {
    "extract": {
      "include": "./**/*.{ts,tsx,json,yaml,txt,md,graphql,cue}",
      "ignore": [
        "./**/node_modules/**",
        "./**/.polywrap/**",
        "./**/__tests__/**"
      ],
      "dir": "./src"
    },
    "inject": {
      "dir": "./readme/sub-sections/base",
      "include": "./**/*.md"
    },
    "outputDir": "./readme/sub-sections/injected"
  }
}